class Solution {
    public void moveZeroes(int[] nums) {
        int zeroCount = 0;
        int indexCount = 0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]==0){
                zeroCount++;
            }
            else{
                nums[indexCount] = nums[i];
                indexCount++;
            }
            
        }
        
        for(int j=0;j<zeroCount;j++){
            nums[indexCount]=0;
            indexCount++;
        }
        }
    }
